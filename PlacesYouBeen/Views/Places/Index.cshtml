@{
  Layout = "_Layout";
}
@using PlacesYouBeen.Models;

<h1>List of Places I've Been</h1>
@if (Model.Count == 0)
{
  <p>Please enter a place that you have been.</p>
}
<ul>
  @foreach (Place item in Model)
  {
    <li>@item.CityName</li>
    <li><img src="@item.Picture"></li>   
  }
</ul>
<a href="/places/new">Add a new place!</a>
